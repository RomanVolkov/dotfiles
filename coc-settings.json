{
    "pyright.enable": true,
    "tsserver.enable": true,
    "languageserver": {
        "python": {
            "command": "pylsp",
            "filetypes": [
                "python"
            ]
        },
        "swift": {
            "command": "sourcekit-lsp",
            "filetypes": [
                "swift"
            ]
        },
        "clangd": {
            "command": "clangd",
            "rootPatterns": [
                "compile_flags.txt",
                "compile_commands.json"
            ],
            "filetypes": [
                "c",
                "cc",
                "cpp",
                "c++",
                "objc",
                "objcpp"
            ]
        }
    },
}
